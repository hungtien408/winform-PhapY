             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="700" d:DesignWidth="800" xmlns:my="clr-namespace:PhapY.uc">
    <Grid>
        <ToolBar Height="35" HorizontalAlignment="Stretch" VerticalAlignment="Top">
            <Button Content="Thống kê hồ sơ theo tháng" Height="23" Name="btnThongKeHoSo" Padding="20,0,20,0" Click="btnThongKeHoSo_Click" />
            <Button Content="Biểu đồ thống kê" Height="23" Name="btnChartHoSo" Padding="20,0,20,0" Click="btnChartHoSo_Click" />
        </ToolBar>
        <Grid Margin="0,32,0,0" Name="Container">
            <my:ThongKeTuThi x:Name="thongKeHoSo1" VerticalAlignment="Stretch" />
        </Grid>
    </Grid>
</UserControl>
